# Ignore Git and CI
.git
.github
.gitignore

# Node/build caches
node_modules
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS/editor junk
.DS_Store
*.swp
.vscode

# PHP vendor cache can be included since already committed; ignore composer artifacts if present
composer.lock
composer.phar
vendor/**/.git

# Tests and docs that aren't needed at runtime
scripts/
docs/
